{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "**/*.jsx",
      "**/*.json",
      "src/generated/**/*",
      /* These are temporary until prettier is upgraded */
      "**/UserContext.tsx",
      "**/OnboardingDialogue.tsx"
    ]
  },
  "rules": {
    "interface-name": false,
    "no-console": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "no-var-requires": false,
    "curly": true,
    "ordered-imports": false
  }
}
